  <div class="row" id="app">
    <div class="col-xs-12">
      <a href="/bank/branch/edit?option=add" class="btn btn-sm btn-primary">
        <i class="icon-plus"></i>
        添加
      </a>
      <div class="space-6"></div>
      <div class="table-header">
        找到{{totalCount}}条数据
      </div>
      <div class="table-responsive">
        <table id="data-list" class="table table-striped table-bordered table-hover no-margin-bottom">
          <thead>
          <tr>
            <th class="center">总行Logo</th>
            <th class="center">支行Logo</th>
            <th class="center">总行编码</th>
            <th class="center">支行编码</th>
            <th class="center">支行名称</th>
            <th class="center">支行地址</th>
            <th class="center">联系人</th>
            <th class="center">电话</th>
            <th class="center">数据状态</th>
            <th class="hidden-480">添加</th>
            <th>
              <i class="icon-time bigger-110 hidden-480"></i>
              添加时间
            </th>
            <th class="hidden-480">更新</th>
            <th>
              <i class="icon-time bigger-110 hidden-480"></i>
              更新时间
            </th>
            <th>操作</th>
          </tr>
          </thead>
          <tbody>
          {{#each dataList}}
            <tr>
              <td class="center">
                <img src="{{this.bankLogo}}" style="width: 120px">
              </td>
              <td class="center">
                <img src="{{this.branchLogo}}" style="width: 120px">
              </td>
              <td>{{this.bankCode}}</td>
              <td>{{this.branchCode}}</td>
              <td>{{this.branchName}}</td>
              <td>{{this.provinceName}}{{this.cityName}}{{this.districtName}}{{this.address}}</td>
              <td>{{this.branchContact}}</td>
              <td>{{this.branchContactCellphone}}</td>
              <td>{{this.dataStatusText}}</td>
              <td>{{this.createUser}}</td>
              <td>{{this.createTime}}</td>
              <td>{{this.updateUser}}</td>
              <td>{{this.updateTime}}</td>
              <td>
                <div class="visible-md visible-lg hidden-sm hidden-xs action-buttons">
                  <button type="button" class="btn btn-sm btn-primary" ng-click="onChange({{this.branchID}})">
                    <i class="icon-edit"></i>
                    修改
                  </button>
                  {{# if this.isNormal}}
                    <button type="button" class="btn btn-sm btn-danger" ng-click="onDelete('{{this.branchID}}','{{this.branchName}}')">
                      <i class="icon-trash"></i>
                      冻结
                    </button>
                  {{/if}}

                  {{# if this.isFrozen}}
                    <button type="button" class="btn btn-sm btn-success" ng-click="onRecover('{{this.branchID}}','{{this.branchName}}')">
                      <i class="icon-reply"></i>
                      恢复
                    </button>
                  {{/if}}
                  {{# if this.isDelete}}
                    <button type="button" class="btn btn-sm btn-success" ng-click="onRecover('{{this.branchID}}','{{this.branchName}}')">
                      <i class="icon-reply"></i>
                      恢复
                    </button>
                  {{/if}}
                </div>
              </td>
            </tr>
          {{/each}}
          </tbody>
        </table>
      </div>
      {{#if paginationArray}}
        <div class="modal-footer no-margin-top">
          <ul class="pagination pull-right no-margin">
            <li class="prev">
              {{#if prePageNum}}
                <a href="/bank/branch?pageNumber={{prePageNum}}">
                <i class="icon-double-angle-left"></i>
                </a>
              {{else}}
                <span>
              <i class="icon-double-angle-left"></i>
            </span>
              {{/if}}
            </li>
            {{#each paginationArray}}
              <li>
                <a href="/bank/branch?pageNumber={{this}}">{{this}}</a>
              </li>
            {{/each}}
            <li class="next">
              {{#if nextPageNum}}
                <a href="/bank/branch?pageNumber={{nextPageNum}}">
                <i class="icon-double-angle-right"></i>
                </a>
              {{else}}
                <span>
              <i class="icon-double-angle-right"></i>
            </span>
              {{/if}}
            </li>
          </ul>
        </div>
      {{/if}}
    </div>
  </div>
</div>

<input type="hidden" value="{{currentPageNum}}" id="hidden-currentPageNum">
<input type="hidden" value="{{dataStatus}}" id="hidden-dataStatus">
<script src="/javascripts/view/bankBranch.js"></script>