<div class="breadcrumbs" id="breadcrumbs">
  <ul class="breadcrumb">
    <li>
      <i class="icon-home home-icon"></i>
      <a href="/index">主页</a>
    </li>
    <li>
      客户基本信息管理
    </li>
    <li class="active">支行账号管理</li>
  </ul>
</div>

<div class="page-content" ng-app="myApp" ng-controller="myCtrl">
  <div class="page-header">
    <h1>
      支行账号管理
      <small>
        <i class="icon-double-angle-right"></i>
        信息查询
      </small>
    </h1>
  </div>

  <div class="row" id="app">
    <div class="col-xs-12">
      <a href="/systemAccount/edit?option=add" class="btn btn-sm btn-primary">
        <i class="icon-plus"></i>
        添加
      </a>
      <div class="space-6"></div>
      <div class="table-header">
        找到{{totalCount}}条数据
      </div>
      <div class="table-responsive">
        <table id="data-list" class="table table-striped table-bordered table-hover no-margin-bottom">
          <thead>
          <tr>
            <th class="center">所属银行</th>
            <th class="center">所属支行</th>
            <th class="center">软件系统</th>
            <th class="center">登陆账号</th>

            <th class="center">数据状态</th>
            <th class="hidden-480">添加</th>
            <th>
              <i class="icon-time bigger-110 hidden-480"></i>
              添加时间
            </th>
            <th class="hidden-480">更新</th>
            <th>
              <i class="icon-time bigger-110 hidden-480"></i>
              更新时间
            </th>
          </tr>
          </thead>
          <tbody>
          {{#each dataList}}
            <tr>
              <td class="center">
                {{this.bankName}}
              </td>
              <td>{{this.branchName}}</td>
              <td>{{this.systemName}}</td>
              <td>{{this.account}}</td>
              <td>{{this.dataStatusText}}</td>
              <td>{{this.createUser}}</td>
              <td>{{this.createTime}}</td>
              <td>{{this.updateUser}}</td>
              <td>{{this.updateTime}}</td>
<!--              <td>-->
<!--                <div class="visible-md visible-lg hidden-sm hidden-xs action-buttons">-->
<!--                  <button type="button" class="btn btn-sm btn-primary" ng-click="onChange({{this.accountID}})">-->
<!--                    <i class="icon-edit"></i>-->
<!--                    修改-->
<!--                  </button>-->
<!--                  {{# if this.isNormal}}-->
<!--                    <button type="button" class="btn btn-sm btn-danger" ng-click="onDelete('{{this.accountID}}','{{this.systemName}}')">-->
<!--                      <i class="icon-trash"></i>-->
<!--                      删除-->
<!--                    </button>-->
<!--                  {{/if}}-->

<!--                  {{# if this.isFrozen}}-->
<!--                    <button type="button" class="btn btn-sm btn-success" ng-click="onRecover('{{this.accountID}}','{{this.systemName}}')">-->
<!--                      <i class="icon-reply"></i>-->
<!--                      恢复-->
<!--                    </button>-->
<!--                  {{/if}}-->
<!--                  {{# if this.isDelete}}-->
<!--                    <button type="button" class="btn btn-sm btn-success" ng-click="onRecover('{{this.accountID}}','{{this.systemName}}')">-->
<!--                      <i class="icon-reply"></i>-->
<!--                      恢复-->
<!--                    </button>-->
<!--                  {{/if}}-->
<!--                </div>-->
<!--              </td>-->
            </tr>
          {{/each}}
          </tbody>
        </table>
      </div>
      {{#if paginationArray}}
        <div class="modal-footer no-margin-top">
          <ul class="pagination pull-right no-margin">
            <li class="prev">
              {{#if prePageNum}}
                <a href="/systemAccount?pageNumber={{prePageNum}}">
                <i class="icon-double-angle-left"></i>
                </a>
              {{else}}
                <span>
              <i class="icon-double-angle-left"></i>
            </span>
              {{/if}}
            </li>
            {{#each paginationArray}}
              <li>
                <a href="/systemAccount?pageNumber={{this}}">{{this}}</a>
              </li>
            {{/each}}
            <li class="next">
              {{#if nextPageNum}}
                <a href="/systemAccount?pageNumber={{nextPageNum}}">
                <i class="icon-double-angle-right"></i>
                </a>
              {{else}}
                <span>
              <i class="icon-double-angle-right"></i>
            </span>
              {{/if}}
            </li>
          </ul>
        </div>
      {{/if}}
    </div>
  </div>
</div>

<input type="hidden" value="{{currentPageNum}}" id="hidden-currentPageNum">
<input type="hidden" value="{{dataStatus}}" id="hidden-dataStatus">
<script src="/javascripts/view/systemAccount.js"></script>